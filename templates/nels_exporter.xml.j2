<?xml version="1.0"?>
<tool name="Send files" id="nels_export" tool_type="data_source" add_galaxy_url="False" force_history_refresh="True" version="0.0.1">
    <description>to NeLS storage</description>
    <command></command>
    <inputs action="{{ nels_storage_url }}" check_values="False" method="post"> 
        <display>go to NELS Data Manager </display>
        <param name="appCallbackUrl" type="baseurl" value="/root?tool_id=nels_exporter_hidden" />
        <param name="appName" type="hidden" value="Galaxy" />
    </inputs>
    <uihints minwidth="800"/>
    <outputs>
        <data name="output" format="auto" />
    </outputs>
    <options sanitize="False" refresh="True"/>
</tool>
